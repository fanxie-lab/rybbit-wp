{
  "name": "rybbit-analytics",
  "version": "1.0.0",
  "description": "Privacy-friendly, cookieless analytics for WordPress",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .js,.vue",
    "lint:fix": "eslint src --ext .js,.vue --fix",
    "format": "prettier --write \"src/**/*.{js,vue,css}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "wp-env": "wp-env",
    "wp:start": "wp-env start",
    "wp:stop": "wp-env stop",
    "wp:destroy": "wp-env destroy",
    "wp:clean": "wp-env clean all",
    "wp:logs": "wp-env logs"
  },
  "keywords": [
    "wordpress",
    "analytics",
    "privacy",
    "cookieless",
    "rybbit"
  ],
  "author": "Rybbit",
  "license": "GPL-2.0-or-later",
  "dependencies": {
    "axios": "^1.6.2",
    "pinia": "^2.1.7",
    "vue": "^3.4.0",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vitest/coverage-v8": "^1.1.0",
    "@vitest/ui": "^1.1.0",
    "@vue/test-utils": "^2.4.3",
    "@vueuse/core": "^14.0.0",
    "@wordpress/env": "^10.0.0",
    "autoprefixer": "^10.4.16",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.19.2",
    "jsdom": "^23.0.1",
    "lucide-vue-next": "^0.554.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "radix-vue": "^1.9.17",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "terser": "^5.0.0",
    "typescript": "^5.9.3",
    "vite": "^5.0.0",
    "vitest": "^1.1.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
